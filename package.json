{
  "name": "myncraft.js",
  "version": "1.0.0",
  "description": "mineflayer based minecraft bot package",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch ./src -e ts --exec 'npm run test' ",
    "test": "clear&&cd src&&node --no-warnings=ExperimentalWarning --loader ts-node/esm test.ts",
    "build": "tsc --project tsconfig.json & tsc --project tsconfig.mjs.json "
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MynCraft/MynCraft.js.git"
  },
  "keywords": [
    "mineflayer",
    "minecraft",
    "myncraftjs",
    "mcbot",
    "minecraftbot",
    "mineflayerjs",
    "senesatka"
  ],
  "author": {
    "email": "senesatka@gmail.com",
    "name": "SeneSatka"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MynCraft/MynCraft.js/issues"
  },
  "homepage": "https://github.com/MynCraft/MynCraft.js#readme",
  "dependencies": {
    "@myncraftjs/logger": "^0.0.4",
    "minecraft-data": "^3.69.0",
    "minecraft-protocol": "^1.47.0",
    "prismarine-nbt": "^2.5.0",
    "prismarine-registry": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.5.3"
  }
}
